{
  "name": "CREATE Law",
  "details": "RA 11534 - Corporate Recovery and Tax Incentives for Enterprises",
  "children": [
            {
              "name": "Domestic Corporations",
              "details": "Changes ",
              "children": [
                                          {
                                        "name": "Regular Corporate Income Tax",
                                          "details": "Changes",
                                        "children":[
                                          
                                                     {
                                        "name": "Before",
                                                                                 "details": "30%",
                                        "children":[]
                                      },  {
                                        "name": "CREATE Law",
                                        "details": "Starting July 2020, 25% or 20%, subject to the following conditions: \n (1) Net taxable income is not more than P5M, and \n (2) Total Assets not more than P100M",
                                        "children":[]
                                      }
                                        ]
                                      }, 
                                          {
                                        "name": "Minimum Corporate Income Tax",
                                        "details": "Changes",
                                        "children":[
                                           {
                                        "name": "Before",
                                        "details": "2%",
                                        "children":[]
                                      },
                                       {
                                        "name": "CREATE Law",
                                        "details": "1%, July 1, 2020 to July 30, 2023",
                                        "children":[]
                                      }
                                        ]
                                      },   
                                      {
                                        "name": "Deductions from Gross Income",
                                        "details": "Changes",
                                        "children":[
                                                                     {
                                        "name": "Before",
                                        "details": "Non-deductible interest expense: 33% of interest income subject to final tax",
                                        "children":[]
                                      },
                                                                 {
                                        "name": "CREATE Law",
                                        "details": "(1) Additional 50% deduction for labor training expense. (2) Non-deductible interest expense: 20% of interest income subject to final tax",
                                        "children":[]
                                      }
                                        ]
                                      },   
                                      {
                                        "name": "IAET",
                                         "details": "Improperly Accumulated Earnings Tax - Changes",
                                        "children":[
                                             {
                                        "name": "Before",
                                         "details": "10%",
                                        "children":[]
                                      },
                                         {
                                        "name": "CREATE Law",
                                         "details": "Repealed, July 01,2020 to June 30, 2023",
                                        "children":[]
                                      }

                                        ]
                                      },   
                                      {
                                        "name": "PEIH",
                                        "details": "Proprietary Educational Institutions and Hospitals - Changes",
                                        "children":[
                                             {
                                        "name": "Before",
                                         "details": "10% of Net taxable income",
                                        "children":[]
                                      },
                                         {
                                        "name": "CREATE Law",
                                         "details": "1% of net taxable income, July 01, 2020 to June 30, 2023",
                                        "children":[]
                                      }
                                        ]
                                      },   
                                      {
                                        "name": "Intercorporate Dividends",
                                        "details": "Changes",
                                        "children":[
                                             {
                                        "name": "Before",
                                         "details": "Dividends from NRFC - 30% RCIT",
                                        "children":[]
                                      },
                                         {
                                        "name": "CREATE Law",
                                         "details": "Dividends from NRFC - exempt from income tax subject to the following conditions: 1) reinvestment in DC, 2) 20% or more ownership, 3) 2+ years holding period",
                                        "children":[]
                                      }
                                        ]
